fourway_df %>% dplyr::select(Log2FC.y) %>% max() -> four_way_plot_max.y
fourway_df %>% dplyr::select(Log2FC.x) %>% min() -> four_way_plot_min.x
fourway_df %>% dplyr::select(Log2FC.y) %>% min() -> four_way_plot_min.y
