color_x_up = "#4d528f",
color_x_down = "#a3a1c3",
color_y_up = "#de5b6c",
color_y_down = "#f3acb0",
color_x = "#9A9ABC",
color_y = "#F6A6A9",
color_both = "black",
color_ignore = "grey",
max_overlaps = 20,
contrast_1 = "Contrast1_Name",
contrast_2 = "Contrast2_Name",
